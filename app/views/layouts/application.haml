!!! Strict
%html{html_attrs}
  %head
    %title Open San Diego 911 Call Mashup
    = stylesheet_link_tag :all
    = javascript_include_tag "http://maps.google.com/maps/api/js?v=3.3&sensor=false"
    = javascript_include_tag "markermanager", "StyledMarker"
    = javascript_include_tag "http://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"
    = javascript_include_tag "jquery.metadata", "jquery.jmapping.min"
    = javascript_include_tag "application"
    = csrf_meta_tag
  %body
    = yield
      